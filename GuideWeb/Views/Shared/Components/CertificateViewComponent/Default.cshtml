@using Dto.ApiWebDtos.ApiToWebDtos.PageComponets;
@using GuideWeb.Helpers;
@model List<CertificateDto>

<div class="container mt-40 mb-40">
    <div class="row justify-content-center g-4">
        @foreach (var certificate in Model)
        {
            <div class="col-2">@*col-6 col-xl-2 col-lg-4 col-md-6 col-sm-8*@
                <img src="@Resource.ImagePath/@certificate.ImagePath" class="img-fluid" alt="@certificate.Title" title="@certificate.Title">
            </div>
        }
        
    </div>
</div>